/* pages/questionnaire/timu1/timu1.wxss */

.questionnaire-timu{
  margin-top: 418rpx;
  font-size: 38rpx;
  text-align: center;
}
.questionnaire-timu-select{
  display: flex;flex-wrap: wrap;padding: 0 54rpx;justify-content: space-between;
}
.questionnaire-timu-select view{
 width: 300rpx;height: 72rpx;font-size: 31rpx;line-height: 72rpx;border-radius: 4rpx;
 background: #fff;opacity: 0.97;  color: rgba(0, 0, 0, 0.9);
 margin-top: 42rpx;
}
.questionnaire-timu-select view.active{
  background: #FFEC4D;
}
.questionnaire-timu-select view::after{
  margin-left: auto;
}

.questionnaire-timu-button {
display: flex;justify-content: space-between;padding:0 54rpx;

position: fixed;width: 100%;bottom: 106rpx;box-sizing: border-box;
}
.questionnaire-timu-button button{
  padding: 0;
  margin: 0;
  width: 300rpx;
  height: 88rpx;
  font-size: 34rpx;
  border: 1rpx solid rgba(255, 255, 255, 0.2);
  color: rgba(255, 255, 255, 0.2);
  background-color: rgba(255, 255, 255, 0);
}
.questionnaire-timu-button button.active{

  background-color: #FFEC4D;
    color: rgba(0, 0, 0, 0.9)
}